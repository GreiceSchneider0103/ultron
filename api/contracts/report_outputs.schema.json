{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "$id": "https://ultron.local/contracts/report_outputs.schema.json",
  "title": "ReportOutputs",
  "type": "object",
  "required": ["market_dashboard", "audit_report"],
  "properties": {
    "market_dashboard": {
      "type": "object",
      "required": ["type", "listings_count", "price_range", "competitor_summary"],
      "properties": {
        "type": { "const": "market_dashboard" },
        "listings_count": { "type": "number" },
        "price_range": { "type": "object" },
        "competitor_summary": { "type": "object" }
      },
      "additionalProperties": true
    },
    "audit_report": {
      "type": "object",
      "required": ["type", "scores", "recommendations", "metadata"],
      "properties": {
        "type": { "const": "audit_report" },
        "scores": { "type": "object" },
        "recommendations": { "type": "array" },
        "metadata": { "type": "object" }
      },
      "additionalProperties": true
    }
  },
  "additionalProperties": true
}
